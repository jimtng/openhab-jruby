<div class="page-versions">
  <ul class="version-switcher">
    <li><a href="/openhab-jruby/<%= stable_version %>" class="version-button stable">Stable <small>(<%= stable_version %>)</small></a></li>
    <li><a href="/openhab-jruby/main" class="version-button main">Latest <small>(main)</small></a></li>
  </ul>
  <% archived_versions = %w[] %>
  <% unless archived_versions.empty? %>
    <div class="archived-versions">
      <div class="dropdown-wrapper">
        <a class="dropdown-title">
          <span class="title">Archived</span>
          <span class="arrow down"></span>
        </a>
        <ul class="nav-dropdown">
          <% archived_versions.each do |version| %>
            <li class="dropdown-item"><a href="/<%= version %>"><%= version %></a></li>
          <% end %>
        </ul>
      </div>
    </div>
  <% end %>
</div>

